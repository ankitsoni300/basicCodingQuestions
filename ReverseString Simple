var str = "my name is ankit soni"

func reverseStr(str : String) -> String{
    let strArr = str.components(separatedBy: " ")
    var tempStr = ""
    for item in strArr{
        tempStr = item + " " + tempStr
    }
    
    return tempStr
}

reverseStr(str: str)


let word = "my name is ankit"
var reversed = ""
for char in word {
    reversed.insert(char, at: reversed.startIndex)
    print(reversed)
}
print(reversed)
