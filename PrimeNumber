func isPrime(num : Int) -> Bool{
    //2, 3, 5, 7, 11
    var flag = 0
    for i in 2...num/2{
        if num % i == 0{
            flag = 1
            break
        }
    }
    
    if num == 1{
        return false
    }else {
        if flag == 0{
            return true
        }else{
            return false
        }
    }
    
}
